<?xml version="1.0" encoding="UTF-8"?>
<AccountingReports xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="reporting_config.xsd">
  <Reports>
    <Report id="Invoice" template="InvoiceTemplate.jasper" >
        <Model id="invoice" class="de.togginho.accounting.model.Invoice" />
    </Report>
    <Report id="Letterhead" template="Letterhead.jasper">
      <Model id="user" class="de.togginho.accounting.model.User" />
    </Report>
    <Report id="CashFlowStatement" template="CashFlowStatement.jasper">
      <Model id="cashFlow" class="de.togginho.accounting.model.IncomeStatement" />
    </Report>
    <Report id="Revenue" template="RevenueTemplate.jasper">
      <Model id="revenue" class="de.togginho.accounting.model.Revenue" />
    </Report>
    <Report id="Expenses" template="ExpensesTemplate.jasper">
      <Model id="expenseCollection" class="de.togginho.accounting.model.ExpenseCollection" />
    </Report>
  </Reports>
</AccountingReports>
